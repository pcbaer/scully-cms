{
    "name": "pcbaer/scully-cms",
    "description": "ScullyCMS is a simple all-in-one Content Management System that does not need a database.",
    "type": "project",
    "keywords": ["cms", "scully", "simple"],
    "homepage": "https://www.hallerbach-it.de",
    "authors": [
        {
            "name": "Sascha Ternes",
            "email": "info@hallerbach-it.de",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "info@hallerbach-it.de",
        "source": "https://github.com/pcbaer/scully-cms",
        "issues": "https://github.com/pcbaer/scully-cms/issues"
    },
    "license": "GPL-3.0-or-later",
    "readme": "README.md",

    "version": "0.0.1",
    "time": "2019-10-19",

    "require": {
        "php": "~7.3.10",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",

        "sensio/framework-extra-bundle": "^5.5",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.3.*",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/expression-language": "4.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.3.*",
        "symfony/framework-bundle": "4.3.*",
        "symfony/http-client": "4.3.*",
        "symfony/intl": "4.3.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "4.3.*",
        "symfony/security-bundle": "4.3.*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/twig-bundle": "4.3.*",
        "symfony/validator": "4.3.*",
        "symfony/web-link": "4.3.*",
        "symfony/yaml": "4.3.*",

        "components/jquery": "~3.3",
        "twbs/bootstrap": "~4.3",
        "tinymce/tinymce": "~5.1"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "4.3.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },

    "conflict": {
        "symfony/symfony": "*"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },

    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    },

    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "create-links": "bin/create-links.sh",
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "@create-links"
        ]
    },
    "scripts-descriptions": {
        "create-directories": "Create data directories.",
        "create-links": "Create Bootstrap CSS & JS links."
    }
}
